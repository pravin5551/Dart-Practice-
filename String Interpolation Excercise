main() {
 double temp = 230;
  int value = 2 ;
  String pizza = 'pizza';
  String pasta = 'pasta';
  
  print('The temperature is $temp' + 'C');
  print("$value plus $value + ${value + value}" );
  print("I like " + pizza +" " + pasta);
  
  //To write long paragraph
  
  print(''' This is the string interpolation example.
  We can just have a look at it 
  jhvsdkj 
  dflksjv 
  aijsdfpo ''');

}
