void main() {
  final person = Person(name :'Pravin', age : 23, height: 1.76);
  person.printDescription();
}

class Person{
  String name;
  int age;
 double height;
  
  Person({
    required this.name,
    this.age=0,
    this.height=0,
    
  });
  
  void printDescription(){
    print("My name is $name ,I'm $age year old, I'm of $height height" );
  }
  
}
  
